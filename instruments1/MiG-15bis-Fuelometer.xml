<?xml version="1.0"?>

<PropertyList>

  <path>MiG-15bis-Fuelometer.ac</path> 

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>FM_Glass</object-name>
  </effect>

  <effect>
    <inherits-from>Aircraft/MiG-15/Models/Effects/instruments1</inherits-from>
    <object-name>FM_Needle</object-name>
    <object-name>FM_Backplate</object-name>
    <object-name>FM_Foreplate</object-name>
  </effect>

  <animation>
    <type>rotate</type>
    <object-name>FM_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/KES-917B/gauge/fuel-l</property>
    <interpolation>
      <entry>
        <ind>-47.619</ind>
        <dep>-10.0</dep>
      </entry>
      <entry>
        <ind>200.0</ind>
        <dep>42.0</dep>
      </entry>
      <entry>
        <ind>400.0</ind>
        <dep>66.0</dep>
      </entry>
      <entry>
        <ind>600.0</ind>
        <dep>90.0</dep>
      </entry>
      <entry>
        <ind>800.0</ind>
        <dep>111.0</dep>
      </entry>
      <entry>
        <ind>1000.0</ind>
        <dep>133.0</dep>
      </entry>
      <entry>
        <ind>1200.0</ind>
        <dep>180.0</dep>
      </entry>
      <entry>
        <ind>1400.0</ind>
        <dep>173.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.009582</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList> 
