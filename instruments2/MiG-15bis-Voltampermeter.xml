<?xml version="1.0"?>
<PropertyList>
  <path>MiG-15bis-Voltampermeter.ac</path> 

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>VA_Glass</object-name>
  </effect>

  <effect>
    <inherits-from>Aircraft/MiG-15/Models/Effects/instruments2</inherits-from>
    <object-name>VA_Needle</object-name>
    <object-name>VA_Backplate</object-name>
  </effect>

  <animation>
    <type>rotate</type>
    <object-name>VA_Needle</object-name>
    <property>systems/electrical-real/amper-norm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0</dep></entry>
      <entry><ind>0.5</ind><dep>40</dep></entry>
      <entry><ind>1.0</ind><dep>80</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.012</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList> 
